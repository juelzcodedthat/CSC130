my_set1 = set()
my_set1.add(1)
my_set1.update('a', 'b', ['ab'], '1')
my_set1.remove('ab')
my_set1.discard('c')
my_set2 = set('abb1ab')
print(f'my_set1: {my_set1}')
print(f'my_set2: {my_set2}')
print(f'Is my_set2 a subset of my_set1: {my_set2.issubset(my_set1)}')
print(f'union: {my_set1.union(my_set2)}')
print(f'intersection: {my_set1.intersection(my_set2)}')
